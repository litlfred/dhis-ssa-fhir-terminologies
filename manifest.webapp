{
  "version": "1.0.1",
  "name": "FHIR DSTU2 Terminology",
  "description": "Simple FHIR DSTU2 Terminology Service for metadata",
  "launch_path": "index.html",
  "developer": {
    "name": "Carl Leitner <litlfred@gmail.com>",
    "url": "http://github.com/litlfred/dhis-web-fhir-terminologies"
  },
  "default_locale": "en",
  "activities": {
    "dhis": {
      "href": "*"
    }
  },
  "script-library": {
    "dependencies": {
      "lib/valueset_read.js": [
        "/apps/dhis-ssa-core-services/lib/Errors.js",
        "/apps/dhis-ssa-metadata-fhir/lib/MetadataFHIR.js"
      ]
    },
    "bindings": {
      "fhir": {
        "dstu2": [
          {
            "script": "lib/valueset_read.js",
            "resource": "ValueSet",
            "operation": "read",
            "example_url": "http://localhost:8080/ohie/fhir/dstu2/terminologies/ValueSet/example"
          }
        ]
      }
    }
  }
}
